    "@grafana/ui": "7.5.11",
    "@types/lodash": "4.14.175",
    "@types/prismjs": "^1.26.0",
    "@types/react-syntax-highlighter": "^15.5.1"
  },
  "resolutions": {
    "rxjs": "6.6.3",
    "ssri": "8.0.1",
    "@grafana/toolkit/*/immer": "8.0.1",
    "@grafana/toolkit/*/*/yargs-parser": "18.1.2",
    "@grafana/toolkit/*/*/minimist": "1.2.3"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
